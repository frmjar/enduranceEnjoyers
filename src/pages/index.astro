import Galeria from '../components/Galeria.astro'
import HeaderExtended from '../components/HeaderExtended.astro'
import Layaut from '../components/Layaut.astro'
import NovedadesLogros from '../components/NovedadesLogros.astro'
import TeamDescription from '../components/TeamDescription.astro'
---
  <Layaut>
    <HeaderExtended id='home' />
    <TeamDescription />
    <NovedadesLogros id='novedades' class='scroll-mt-20' />
